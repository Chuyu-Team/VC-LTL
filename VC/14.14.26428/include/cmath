_Check_return_ inline float pow(_In_ float _Xx, _In_ int _Yx) _NOEXCEPT
	{
	if (_Yx == 2)
		return (_Xx * _Xx);

	return (_CSTD powf(_Xx, static_cast<float>(_Yx)));
	}

